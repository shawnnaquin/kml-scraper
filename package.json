{
  "name": "xml",
  "version": "1.0.0",
  "description": "",
  "main": "scripts/county.js",
  "config": {

    "DISTFOLDER": "dist",
    "DISTTEMPFOLDER": "temp",
    "DISTCOMPLEXFOLDER": "complex",
    "DISTSIMPLEFOLDER": "simplified",

    "PRECISION": 0.001,
    "GEOPRECISION": 6,

    "SRCFOLDER": "src",
    "SRCDMA": "nielsentopo.json",
    "SRCDMAINFO": "dmainfo.json",
    "SRCABBREV": "stateabbrev.json",
    "SRCFIPS": "statefips.json",
    "SRCZCTA": "cb_2017_us_zcta510_500k.kml",
    "SRCCOUNTY": "cb_2017_us_county_20m.kml",
    "SRCCITYFOLDER": "citykml",
    "SRCNEIGHBORHOODFOLDER": "neighborhoodshp"

  },
  "scripts": {

    "test": "du -hsc dist/simplified",

    "process:county": "node scripts/process.js --county",
    "process:zip": "node scripts/process.js --zip",
    "process:city": "node scripts/process.js --city",
    "process:neighborhood": "node scripts/process.js --neighborhood",

    "simplify:zip": "bash ./scripts/simplify.sh zip kml",
    "simplify:county": "bash ./scripts/simplify.sh county kml",
    "simplify:dma": "bash ./scripts/simplify.sh dma json",
    "simplify:city": "bash ./scripts/simplify.sh city kml",
    "simplify:neighborhood": "bash ./scripts/simplify.sh 'neighborhood' 'kml'",

    "features:dma": "node scripts/featureCollection2features.js --dma",
    "topo:dma": "bash ./scripts/topo2geo.sh 'dma'",

    "shp:neighborhood": "bash ./scripts/shp2kml.sh 'neighborhood' 'shp'",

    "neighborhood": "npm run shp:neighborhood && npm run process:neighborhood && npm run simplify:neighborhood",
    "dma": "npm run topo:dma && npm run features:dma && npm run simplify:dma",
    "city": "npm run process:city && npm run simplify:city",
    "zip": "npm run process:zip && npm run simplify:zip",
    "county": "npm run process:county && npm run simplify:county",

    "warning": "echo \"WARNING: YOU'RE CRAZY!\n\n\"",
    "all": "npm run warning && npm run neighborhood && npm run dma && npm run city && npm run zip && npm run county"

  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fs-extra": "^7.0.1",
    "fse": "^4.0.1",
    "path": "^0.12.7",
    "read-xml": "^3.0.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "install": "^0.12.2",
    "node-gyp": "^4.0.0",
    "npm": "^6.9.0",
    "readdirp": "^3.0.0"
  }
}
